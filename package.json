    "tsx": "^4.7.1",
    "typescript": "~5.7.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.25.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "packageManager": "pnpm@10.11.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2"
    }
